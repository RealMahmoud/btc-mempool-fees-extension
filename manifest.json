{
  "name": "BTC mempool fees Extension",
  "version": "1.1",
  "description": "Displays the current mempool fees price using your preferred mempool fees provider.",
  "manifest_version": 2,
  "background": {
    "page": "src/background.html",
    "persistent": false
  },
  "icons": {
    "64": "static/Bitcoin.png"
  },
  "browser_action": {
    "default_icon": "static/Bitcoin.png",
    "default_title": "BTC mempool fees",
    "default_popup": "src/popup.html"
  },
  "permissions": [
    "alarms",
    "storage"
  ],
  "applications": {
    "gecko": {
      "id": "{5090ef94-9d8b-45ee-a331-795973b87584}",
      "strict_min_version": "57.0"
    }
  },
  "options_ui": {
    "page": "src/options.html",
    "open_in_tab": true
  }
}
